{
  "name": "log-service-typescript-client",
  "version": "1.0.0",
  "description": "TypeScript gRPC client for log service",
  "main": "dist/client.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/client.js",
    "dev": "ts-node src/client.ts",
    "generate-proto": "grpc_tools_node_protoc --proto_path=../../proto --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --ts_out=grpc_js:./src/generated ../../proto/log_service.proto",
    "clean": "rm -rf dist src/generated"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.9",
    "@grpc/proto-loader": "^0.7.9",
    "google-protobuf": "^3.21.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/google-protobuf": "^3.15.6",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "grpc",
    "typescript",
    "logging",
    "client"
  ],
  "author": "",
  "license": "MIT"
}
